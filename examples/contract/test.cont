// table
table someTable:
        hdr.ethernet.srcAddr == 16w8 ->
                mac_forward,
                (
                        port : < bs[3], { (0, 2) -> ([0,0], low) } > ,
                        address : < bs[3], { (0, 2) -> ([1,1], high) } > 
                )
        hdr.ethernet.srcAddr == 16w9 ->
                drop, ()
